<!doctype html>
<html lang="en">
  <head>
    <%- include ("./partials/head.ejs") %>
  </head>
  <body>






     <!-- <-----------------NAVBAR----------------------------------> 


     <%- include ("./partials/header.ejs") %>



<!-- <-----------------------LOGIN--------------------------------------- -->



<section id="contact" class="md-padding">
	<div class="container">

			<div class="row text-center">
				<div class="col-md-4 offset-md-4">
					<div class="section-header">
						<img src="/images/NABI.png" alt="" width="200" height="100">
					</div>
				</div>
			</div>

		<div class="row">
			<div class="col-lg-12">
				<form id="contactForm" action="/login" method="POST" name="sentMessage" novalidate="novalidate">
					<div class="row">
						<div class="col-md-6 offset-3">
							<div class="form-group">
								<input class="form-control" id="email" type="email" name="email" placeholder="Your Email *" required="required" data-validation-required-message="Please enter your name.">
								<p class="help-block text-danger"></p>
							</div>
							<div class="form-group">
								<input class="form-control" id="password" type="password"name="password" placeholder="Your Password *" required="required" data-validation-required-message="Please enter your password.">
								<p class="help-block text-danger"></p>
							</div>
							
						</div>
						
						<div class="clearfix"></div>
						<div class="col-md-6 offset-3">
							<div id="success">
							<button id="sendMessageButton" class="main-btn btn-lg btn-block " type="submit">LOGIN</button>
                        </div>
                    </div>
					</div>
				</form>
			</div>
		</div>

	</div>
</section>

 


<!--==========================
    Footer  Section
============================-->

<%- include ("./partials/footer.ejs") %>

<!-- Optional JavaScript -->
<%- include ("./partials/jsdefault.ejs") %>
  </body>
</html>